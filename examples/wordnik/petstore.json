{
  "swagger": 2.0,
  "info": {
    "version": "1.0.0",
    "title": "Swagger Petstore"
  },
  "host": "http://petstore.swagger.wordnik.com",
  "basePath": "/api",
  "schemes": [
    "http"
  ],
  "paths": {
    "/pets": {
      "get": {
        "summary": "finds pets in the system",
        "responses": {
          "200": {
            "description": "pet response",
            "type": "array",
            "items": {
              "$ref": "#/models/Pet"
            }
          },
          "default": {
            "description": "unexpected error",
            "$ref": "#/models/ErrorModel"
          }
        }
      }
    }
  },
  "models": {
    "Pet": {
      "required": [
        "id",
        "name"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "name": {
          "type": "string"
        },
        "tag": {
          "type": "string"
        }
      }
    },
    "ErrorModel": {
      "required": [
        "code",
        "message"
      ],
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        }
      }
    }
  }
}